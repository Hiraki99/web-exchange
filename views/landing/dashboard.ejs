<% layout ("../layout/layout_landing_page") -%>
    <button class="btn" href='javascript:void(0)' onclick='getHTML()'>login</button>


    <div id="kaka">
    </div>

    <script>
        function getHTML(){
            $.ajax({
                url: "/account/sendfile",
                type: 'post',
                // Fetch the stored token from localStorage and set in the header
                headers: {
                    "x-access-token": localStorage.getItem("token")
                }
            }).done(function(data) {
                console.log(data)
                $("#kaka").html(data)
            
        });;

    }
    </script>