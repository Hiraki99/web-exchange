                            <button name="yes" id="button_newaddress_eth" class="btn btn-primary btn-condition" style="display: none" onclick="createNewAddress('ETH')">Yes</button>
                            <button name="yes" id="button_newaddress_vnd" class="btn btn-primary btn-condition" style="display: none" onclick="redirect('/wallet-currency')">Redirect</button>
                            <button name="cancel" class="btn" data-dismiss="modal">Cancel</button>
                        </div>
                    </div>
                </div>
            </div>

