                                            <li class="bitex-display-if-broker" id="menu-admin_view">
                                                <a href="" data-switch-view="admin_view">
                                                    <i data-switch-view="admin" class="glyphicon glyphicon-user"></i> Admin
                                                </a>
                                            </li>
                                            <li role="presentation" id="menu-otc">
                                                <a onclick="redirect('admin/withdraw')" data-switch-view="trading">
                                                    <i class="glyphicon glyphicon-random"></i> Withdraw
                                                </a>
                                            </li>
                                            <li role="presentation" id="menu-offerbook">
                                                <a onclick="redirect('admin/deposit')" data-switch-view="trading">
