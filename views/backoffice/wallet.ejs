<div id="new_account_currency" class="modal fade" role="dialog" tabindex="0" style="left: 0px; top: 1em;">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header modal-title-draggable">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" data-dismiss="modal">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title">Add New Wallet VND</h4>
            </div>
            <div class="modal-body">
                <div id="12a3euaj8xzn_form" class="form-horizontal uniForm" data-deposit-status="prepare" data-uniform-control-holder-class="form-group">

                    <div class="withdraw-methods">
                        <div class="withdraw-method" data-withdraw-method="TestMethod">
                            <div class="form-group">
                                <label class="col-xs-3 col-sm-4 control-label">Account number</label>
                                <div class="col-xs-6 col-sm-8">
