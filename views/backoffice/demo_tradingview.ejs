<!doctype html>
<html>

<head>
    <style>
        html,
        body,
        #market_trading_view {
            width: 100%;
            height: 100%;
            margin: 0;
            padding: 0;
        }
        a,a:hover{
            cursor: pointer !important;
            padding: 10px;
            border: 1px solid #2d2d2d;
            margin-top: 10px;
        }
    </style>
</head>

<body>
    <div id="market_trading_view">
        <script type="text/javascript" src="https://s3.amazonaws.com/tradingview/tv.js"></script>
        <script type="text/javascript">
            new TradingView.widget({
                "autosize": true,
                "symbol": "BINANCE:BTCUSDT",
                "interval": "30",
                "timezone": "exchange",
                "theme": "#999",
                "toolbar_bg": "#f4f7f9",
                "hide_side_toolbar": false,
                "watchlist": [
                    "BINANCE:BTCUSDT",
                    "BINANCE:ETHBTC"
                ],
                "details": true,
                "locale": "en_US",
                "hideideas": true
            });
        </script>
        

    </div>
    <div>
        <a href="/chart/chart_any">Demo Anychart</a>
        <a href="/chart/chart_tradingview">Demo Tradingview</a>
    </div>
   
</body>

</html>